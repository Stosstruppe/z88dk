	org	0010h

	ld	e, 98
	ld	c, -88
	ld	b, b
	call	mul8s
	ld	c, c
	ld	(0000h), hl
	ret

; hl = e * c
mul8s:
	ld	hl, 81h
	ld	d, 80h
	ld	a, e
	xor	d
	ld	e, a
				; 1st
	sla	c
	jr	c, L1
	ld	a, d
L1:
	cpl
	ld	l, a
				; 2-8
	ld	b, 7
L3:
	add	hl, hl
	ld	a, e
	sla	c
	jr	c, L4
	ld	a, d
L4:
	add	a, l
	ld	l, a
	jr	nc, L5
	inc	h
L5:
	djnz	L3

	ld	a, h
	add	a, 81h
	ld	h, a
	ret
